int i;
int j;
int temp;
int num3[5];

num3[0] = 2;
num3[1] = 5;
num3[2] = 1;
num3[3] = 7;

i = 0;
while (i < 4) {
 j = 0;
 while (j < 4-i) {
   if (num3[j] > num3[j + 1]) {
     temp = num3[j];
     num3[j] = num3[j+1];
     num3[j] = temp;
       
				       
   }
		  
   j = j + 1;
 }
 i = i + 1;
}

